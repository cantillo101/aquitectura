package programacion;

public class operacion {
		int x,y;  
		public static void main (String m[]){
			
				operacion t=new operacion(); 
				t.x=7;
				t.y=8;
				t.suma(); 
		
		
				operacion n=new operacion(); 
				n.x=7;
				n.y=8;
				n.multiplicar(); 
		
		
		
		
		
		}
		public void multiplicar () {
			int r; 
			n= x*y; 
			System.out.println("la suma es" + r);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
}
